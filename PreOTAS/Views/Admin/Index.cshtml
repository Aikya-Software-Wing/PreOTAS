@model IEnumerable<PreOTAS.Models.STUDENT>

@{
    ViewBag.Title = "Index";
}

<h2>PASSWORD LIST</h2>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.USN)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NAME)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DEPT)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Sem)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Section)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Password)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.USN)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NAME)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DEPT.DeptName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Sem)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Section)
        </td>
        <td>
            <span class="test" style="font-family: monospace;">
                @Html.DisplayFor(modelItem => item.Password)
             </span>
</td>
    </tr>
}

</table>
