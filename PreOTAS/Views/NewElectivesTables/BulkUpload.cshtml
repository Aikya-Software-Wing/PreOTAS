
@{
    ViewBag.Title = "BulkUpload";
}

<h2>BulkUpload</h2>

@using (Html.BeginForm("BulkUpload", "NewElectivesTables", FormMethod.Post, new { enctype = "multipart/form-data", @class = "form-horizontal" }))
{
    <div class="form-group">
        File
        <div class="col-md-10">
            <input type="file" name="postedFile" />
        </div>
    </div>

    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="Create" class="btn btn-default" />
        </div>
    </div>
}